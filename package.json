    "license": "MIT",
    "scripts": {
        "start": "nodemon ./local.ts",
        "build": "npx tsc -p .",
        "format": "prettier --write \"**/*.+(js|ts|json)\"",
        "lint": "eslint --fix --ext \"**/*.{ts,tsx}\"",
